{
  "name": "gamenight-client",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@apollo/react-common": "^3.1.4",
    "@apollo/react-components": "^4.0.0",
    "@apollo/react-hoc": "^4.0.0",
    "@apollo/react-hooks": "^4.0.0",
    "emotion": "^10.0.27",
    "graphql": "^15.4.0",
    "graphql.macro": "^1.4.2",
    "heroicons-react": "^1.3.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hook-form": "^5.7.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "tailwindcss": "^1.9.6",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "start": "tailwind build src/index.css -o src/tailwind.css && react-scripts start",
    "build": "tailwind build src/index.css -o src/tailwind.css && react-scripts build",
    "eject": "react-scripts eject",
    "gen": "graphql-codegen --config codegen.yml",
    "watch:gen": "npm run gen -- -w"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "https://localhost:4000",
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.19.11",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-router": "^5.1.9",
    "@types/react-router-dom": "^5.1.7"
  }
}
