{
  "name": "gamenight-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/server.js",
    "watch": "nodemon -e ts,json,js --exec \"node --inspect=0.0.0.0 -r ts-node/register src/server.ts\"",
    "build": "tsc",
    "gen": "graphql-codegen --config codegen.yml",
    "watch:gen": "npm run gen -- -w"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-document-nodes": "1.17.9",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-resolvers": "^1.18.1",
    "@types/graphql": "^14.5.0",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.14.16",
    "@types/passport": "^1.0.5",
    "eslint": "^7.16.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@graphql-codegen/typescript-mongodb": "1.17.9",
    "@graphql-tools/graphql-file-loader": "^6.2.6",
    "@graphql-tools/json-file-loader": "^6.2.6",
    "@graphql-tools/load": "^6.2.5",
    "@graphql-tools/schema": "^7.1.2",
    "apollo-server-fastify": "3.0.0-alpha.3",
    "bcryptjs": "^2.4.3",
    "discord-oauth2": "^2.6.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.9.2",
    "fastify-jwt": "^2.2.0",
    "graphql": "^15.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.3",
    "passport": "^0.4.1",
    "passport-discord": "^0.1.4",
    "passport-github2": "^0.1.12",
    "passport-google": "^0.3.0"
  }
}
